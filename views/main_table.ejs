<% include ../partials/header %>
 <div class="container">
	<div class="row">
			<div class="col-12">
				<div id="content">
				<h1>DashList</h1>
			    <h3> Welcome </h3> 
				<hr>
				</div>
			</div>

			<table class="table table-dark">
				  <thead>
				    <tr>
				      <th scope="col">#</th>
				      <th scope="col">Name</th>
				      <th scope="col">Beer</th>
				      <th scope="col">Wine</th>
				      <th scope="col">Liquor</th>
				    </tr>
				  </thead>
				  <tbody>
				  	<% attendees.forEach(function(attendee, index){ %>
				    <tr>
				      <th scope="row"><%= index+1 %></th>
				    		
				    		<td><%= attendee.name %></td>
				    		<td><%= attendee.beer %></td>
				    		<td><%= attendee.wine %></td>
				    		<td><%= attendee.liquor %></td>				   		
				    </tr>
				    <% }) %>
				
				  </tbody>
				</table>
			
	</div>
</div>





<% include ../partials/footer %>
